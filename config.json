{
  "server": {
    "listenAddress": ":8080",
    "logRequests": true,
    "timeout": 60
  },
  "cache": {
    "directory": "./cache",
    "maxSize": 10240,
    "sizeUnit": "MB",
    "enabled": true,
    "lru": true,
    "cleanOnStart": false,
    "validationCacheTTL": 300
  },
  "repositories": [
    {
      "origin": "archive.ubuntu.com",
      "path": "/ubuntu",
      "enabled": true
    }
  ],
  "version": "1.0.0"
} 