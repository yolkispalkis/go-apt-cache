{
  "server": {
    "listenAddress": ":8080",
    "unixSocketPath": "/run/go-apt-cache/go-apt-cache.sock",
    "unixSocketPermissions": "0660",
    "requestTimeout": "30s",
    "shutdownTimeout": "15s",
    "idleTimeout": "120s",
    "readHeaderTimeout": "10s",
    "maxConcurrentFetches": 20
  },
  "cache": {
    "directory": "/var/cache/go-apt-cache",
    "maxSize": "20GB",
    "enabled": true,
    "cleanOnStart": false,
    "negativeCacheTTL": "5m",
    "metadataBatchInterval": "30s",
    "bufferSize": "64KB",
    "overrides": [
      {
        "pathPattern": "dists/*/InRelease",
        "ttl": "5m"
      },
      {
        "pathPattern": "**/*.deb",
        "ttl": "720h"
      }
    ]
  },
  "logging": {
    "level": "info",
    "file": "/var/log/go-apt-cache/go-apt-cache.log",
    "maxSizeMB": 100,
    "maxBackups": 5,
    "maxAgeDays": 30
  },
  "repositories": [
    {
      "name": "ubuntu",
      "url": "http://archive.ubuntu.com/ubuntu/",
      "enabled": true
    },
    {
      "name": "debian",
      "url": "http://deb.debian.org/debian/",
      "enabled": true
    },
    {
      "name": "raspbian",
      "url": "http://archive.raspberrypi.org/raspbian/",
      "enabled": false
    }
  ]
}