{
  "server": {
    "listenAddress": ":8080",
    "logRequests": true,
    "timeout": 30
  },
  "cache": {
    "directory": "./cache",
    "maxSize": 1024,
    "sizeUnit": "MB",
    "enabled": true,
    "lru": true,
    "cleanOnStart": false,
    "validationCacheTTL": 300
  },
  "repositories": [
    {
      "url": "http://archive.ubuntu.com/ubuntu",
      "path": "/ubuntu",
      "enabled": true
    },
    {
      "url": "http://deb.debian.org/debian",
      "path": "/debian",
      "enabled": true
    },
    {
      "url": "http://security.ubuntu.com/ubuntu-security",
      "path": "/ubuntu-security",
      "enabled": true
    }
  ],
  "version": "1.0.0"
} 